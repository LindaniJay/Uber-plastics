{
  "name": "uber-plastics",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:optimize": "node scripts/optimize-dev.js && next dev",
    "dev:no-csp": "node dev-no-csp.js",
    "dev:fix": "node scripts/fix-permissions.js && next dev",
    "fix-permissions": "node scripts/fix-permissions.js",
    "build": "set ESLINT_NO_DEV_ERRORS=true && next build",
    "build:analyze": "ANALYZE=true next build",
    "build:optimize": "node scripts/optimize-build.js",
    "start": "next start",
    "lint": "echo 'Linting disabled for now'",
    "lint:fix": "echo 'Linting disabled for now'",
    "perf": "npm run build:optimize",
    "perf:optimize": "node scripts/performance-optimization.js",
    "perf:test": "npm run build && npm run perf:optimize",
    "perf:validate": "node scripts/validate-performance.js",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "baseline-browser-mapping": "^2.8.18",
    "canvas-confetti": "^1.9.3",
    "critters": "^0.0.23",
    "framer-motion": "12.23.24",
    "lucide-react": "^0.460.0",
    "next": "^15.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.4.1",
    "react-webcam": "^7.2.0",
    "recharts": "2.12.7",
    "swr": "2.2.4",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.56.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "20.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "autoprefixer": "10.4.21",
    "browserslist": "^4.26.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.0.0",
    "madge": "^5.0.0",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  }
}